      <body>
        <div id="root"><div><!-- Drawer Slider Out -->
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/drawer/3.2.2/css/drawer.min.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/iScroll/5.2.0/iscroll.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/drawer/3.2.2/js/drawer.min.js"></script>

<style>
body:not(.drawer) .drawer-nav {
  display: none !important;;
}

header:not(.site-header) {
  display: none !important;
}

.site-header {
  font-family: 'Assistant', sans-serif !important;
  padding: 10px 10px;
  display: flex;
  flex-direction: column;
  z-index: 100;
  position: relative;
  background-image: url(/images/img_2guy87UBVYiehKmbq54DgL/light_wool_%402x.png?fit=inside&w=1600);
  border-bottom: 2px solid #ddd;
}

main {
  padding-top: 0 !important;
}

#site-logo {
  height: 60px;
  width: auto;
}

.site-header__desktop-nav {
  flex: 1;
  display: none;
  height: 112px;
  margin-top: 5px;
}

.site-header__desktop-nav ul {
  flex: 1;
  display: flex;
  list-style: none;
  margin: 0;
  align-items: center;
  justify-content: center;
}

.site-header__desktop-nav li,
.top-nav a {
  transition: background 100ms, color 100ms;
  border-radius: 4px;
}

.top-nav a {
  display: inline-block;
}

.site-header__desktop-nav li:hover,
.member-login > a {
  background: rgba(0, 0, 0, 0.1);
}

.member-login:hover > a {
  color: rgba(255,255,255,0.9) !important;
  background: rgba(0, 0, 0, 1);
}

.member-login > a {
  font-size: 11px;
}

.site-header__desktop-nav li a,
.top-nav > a,
.member-login > a {
  padding: 0.5rem 1rem 0.4rem !important;
  font-weight: 700 !important;
  display: inline-block;
  color: rgba(0, 0, 0, 0.88);
  text-decoration: none;
  text-transform: uppercase;
  letter-spacing: 1px;
}

.drawer-menu {
  padding: 1.5rem;
}

button.drawer-toggle {
  margin-top: 14px;
