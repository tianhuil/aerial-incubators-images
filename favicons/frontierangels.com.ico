<script>
$(document).ready(function() {
    if (window.parent !== window) {
      function fixLinks() {
        $('header a, footer a').each(function(_, el) {
          var $el = $(el);
          if (!/viewport=/.test($el.attr('href')) && !/^tel/.test(($el.attr('href')))) {
            var url = new URL($el.attr('href'), window.location.origin);
            url.searchParams.append('viewport', 'true');
            $el.attr('href', url.toString());
          }
        });
      }
      fixLinks();
      setInterval(fixLinks, 1000);
    }
});
</script>


<!-- Image Rotator Component -->

<script type="module">
  import {
    LitElement,
    css,
    html,
  } from "https://unpkg.com/lit-element@2.3.1/lit-element.js?module";
  import { repeat } from "https://unpkg.com/lit-html@1.2.1/directives/repeat.js";
  
  class ImageRotator extends LitElement {
    static get properties() {
      return {
        interval: { type: Number },
        src: { type: String }
      };
    }
  
    static get styles() {
      return css`
        img {
          background: white;
          max-width: 100%;
          box-shadow: 0 0 8px rgba(0,0,0,0.2);
          position: absolute;
          left: 50%;
          top: 50%;
          transform: translateX(-50%) translateY(-50%);
          transition: opacity 1s;
          opacity: 0;
        }

        img.visible {
          opacity: 1;
        }
      `;
    }
  
    constructor() {
      super();
      this.src = '';
      this.timer = null;
      this.rotate = this.rotate.bind(this);
      this.index = 0;
      this.interval = 5000;
    }
  
    connectedCallback() {
      super.connectedCallback();
      this.timer = setInterval(this.rotate, this.interval);
      setTimeout(() => {
        this.rotate();
      }, 16);
    }

    disconnectedCallback() {
      if (this.timer) {
        clearInterval(this.timer);
      }
    }

    render() {
      const images = this.src
        .split(',')
        .map(src => src.trim())
        .map((src, i) => html`<img src="${src}" alt="" />`);
  
      return html`
        ${images}
      `;
    }

    rotate() {
      const images = this.shadowRoot.querySelectorAll('img');
      
      if (images.length <= 0) {
        return;
      }

      for (let i = 0; i < images.length; i++) {
        images[i].classList.remove('visible');
      }
      
      images[this.index].classList.add('visible');

      this.index += 1;
      if (this.index >= images.length) {
        this.index = 0;
      }
    }
  }
  
  customElements.define("fa-image-rotator", ImageRotator);
</script>
  
<style>
  fa-image-rotator {
    display: block;
  }

  @media screen and (min-width: 1200px) {
    fa-image-rotator {}
  }
</style>

<!-- End Image Rotator -->

