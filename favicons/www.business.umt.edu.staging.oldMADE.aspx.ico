    <script>
        var newsList = "";
        var newsItem = "";
        $.ajax({
            url: "https://www.umt.edu/business/stories/default.json.php",
            dataType: "json",
            async: false,
            cache: false,
            success: function(data) {
                $.each(data.posts, function(i, v) {
                    newsItem = '';
                    newsItem += '<li class="col-md-4 news-item">';
                    newsItem += '<a href="'+v.link+'" target="_parent">';
                    if(v.image === undefined){
                        newsItem += '<img src="https://www.umt.edu/_files/images/news-img.jpg" alt="'+v.title+'" class="mb-3 w-100 d-none d-md-block" />';
                    }else{
                        newsItem += '<img src="'+v.image+'" alt="'+v.imageAltText+'" loading="lazy" class="mb-3 w-100 d-none d-md-block" />';
                    }
                    newsItem += '<div class="fs-19">'+v.title+'</div>';
                    newsItem += ' </a>';
                    newsItem += '</li>';
                    newsList += newsItem;
                    return i < (3 - 1);
                });
                    $("#jsonnewsblockr11-cob-news").prepend(newsList);
            },
            error: function(data) {
                if (data.status == 200) {
                    console.log("JSON Not Valid: https://www.umt.edu/business/stories/default.json.php");
                } else {
                    console.log("URL Not Found: https://www.umt.edu/business/stories/default.json.php");
                }
            }
        });
    </script>
    
                            
    
                            
    
                


	
	
